@model List<ExchangeViewModel.Exchange_Rates>
@{
    ViewData["Title"] = "Exchange";
    Layout = "~/Views/UserLayout/Index.cshtml";
}


<div class="form-element-area">
	<div class="container">
		<div class="row">
			<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
				<div class="form-element-list">
					<div class="basic-tb-hd">
						<h2>Role Listesi Sayfası</h2>

					</div>

					<div class="row">

						<table class="table table-bordered">
							<tr>
								 
								<th>Döviz Kodu</th>
								<th>Birim Fiyat</th>  
							</tr>
							@foreach (var i in Model)
							{

								<tr>
									<td>@i.currency</td>
									<td>@i.exchange_rate_buy</td>
									 

								</tr>
							}
						</table>

					</div>

				</div>
			</div>
		</div>

	</div>
</div>



